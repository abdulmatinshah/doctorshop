<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(quotation) -%>">
  <span class='title'>
    <%= quotation.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_quotations_admin_quotation_path(quotation),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.quotations_admin_quotation_path(quotation),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => quotation.name),
        :method => :delete %>
  </span>
</li>
